{
  "name": "blinktrade-kycform",
  "description": "BlinkTrade KYC Form",
  "scripts": {
    "start": "cross-env NODE_ENV=production BABEL_ENV=node node index.js",
    "dev": "cross-env NODE_ENV=development BABEL_ENV=node node index.js",
    "build": "cross-env NODE_ENV=production webpack -p",
    "lint": "$(npm bin)/eslint app",
    "manage:translations": "$(npm bin)/babel app --plugins react-intl app/**/*.js && babel-node ./app/utils/translationRunner.js",
    "deploy": "gcloud app deploy"
  },
  "engines": {
    "node": ">=6.9.1",
    "npm": "3.10.8"
  },
  "dependencies": {
    "@google-cloud/storage": "0.3.0",
    "aphrodite": "^1.1.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-react-intl": "^2.2.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "body-parser": "1.15.2",
    "cross-env": "^3.1.3",
    "express": "4.14.0",
    "intl": "^1.2.5",
    "json-loader": "^0.5.4",
    "multer": "1.2.0",
    "node-fetch": "^1.6.3",
    "qs": "^6.3.0",
    "ramda": "^0.22.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.6",
    "react-signature-pad": "0.0.6",
    "redux": "^3.6.0",
    "redux-actions": "^1.1.0",
    "redux-form": "^6.2.0",
    "redux-thunk": "^2.1.0",
    "sjcl": "^1.0.6",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "webpack-isomorphic-tools": "^2.6.4"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.11.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1",
    "react-intl-translations-manager": "^3.1.0"
  }
